/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@models/*": [
        "src/app/shared/models/*"
      ],
      "@contracts/*": [
        "src/app/shared/contracts/*"
      ],
      "@services/*": [
        "src/app/shared/services/*"
      ],
      "@flight/*": [
        "src/app/components/flight/*"
      ],
      "@environments/*": [
        "src/environments/*"
      ]
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
